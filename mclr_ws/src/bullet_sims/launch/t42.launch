<launch>
    <!-- Load the urdf into the parameter server. -->
    <node name="standing" pkg="bullet_sims" type="02_one_leg_stand.py" output="screen"/>
    
    <param name="robot_description" command ="cat $(find talos_description)/robots/talos_reduced_no_hands.urdf" />

    <node pkg="robot_state_publisher" type="robot_state_publisher" name="robot_state_pub" >
    </node>

    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find bullet_sims)/rviz/t41.rviz" />
</launch>